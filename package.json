{
  "name": "shopifyauth",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pretty": "prettier --write ./",
    "update": "ncu -u",
    "update:url": "node _developer/updateDashboard.js",
    "ngrok:auth": "ngrok authtoken <auth-token-goes-here>",
    "ngrok": "ngrok http 3000",
    "-----> Shopify <-----": "",
    "g:install": "npm i -g @shopify/app@latest @shopify/cli@latest",
    "shopify": "shopify",
    "-----> Database <-----": "",
    "pg:create": "mkdir database; pg_ctl -D database init",
    "pg:start": "pg_ctl -D database start",
    "pg:stop": "pg_ctl -D database stop",
    "-----> Prisma <-----": "",
    "prisma": "npx prisma",
    "prisma:push": "npx prisma db push",
    "prisma:pull": "npx prisma db pull",
    "-----> Reserved Scripts <-----": "",
    "prepare": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@shopify/cli-kit": "^3.52.0",
    "@shopify/polaris": "^12.5.0",
    "@shopify/shopify-api": "^8.1.1",
    "cryptr": "^6.3.0",
    "dotenv": "^16.3.1",
    "next": "14.0.4",
    "next-api-middleware": "^2.0.1",
    "ngrok": "^5.0.0-beta.2",
    "npm-check-updates": "^16.14.12",
    "prettier": "^3.1.1",
    "prisma": "^5.7.0",
    "react": "^18",
    "react-dom": "^18"
  }
}
